{
  "name": "peg-slam-mobile",
  "version": "1.0.0",
  "description": "Peg Slam - Mobile App",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "build": "expo export --platform web",
    "web": "npx expo start --web --clear --port 5000",
