  "dependencies": {
    "@expo/metro-runtime": "~4.0.0",
    "@react-native-async-storage/async-storage": "2.1.0",
    "axios": "1.7.0",
    "expo": "~54.0.0",
    "expo-av": "~16.0.0",
    "expo-image-picker": "~17.0.0",
    "expo-status-bar": "~3.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.0",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "http-server": "^14.1.1"
  },
  "private": true
}
